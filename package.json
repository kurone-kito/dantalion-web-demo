{
  "name": "dantalion-web-demo",
  "version": "0.20.0",
  "private": true,
  "description": "The playground web app that calculates the personality from the birthday",
  "keywords": [
    "ba-zi",
    "bazi",
    "birthday",
    "chinese-astrology",
    "console",
    "console-app",
    "four-pillars",
    "library",
    "self-assessment",
    "terminal-app",
    "ziweidoushu",
    "zodiac"
  ],
  "homepage": "https://kurone-kito.github.io/dantalion/",
  "bugs": {
    "url": "https://github.com/kurone-kito/dantalion-web-demo/issues"
  },
  "repository": "github:kurone-kito/dantalion-web-demo",
  "license": "MIT",
  "author": "kurone-kito <krone@kit.black> (https://kit.black/)",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "echo \"Dry run: build\"",
    "build:re": "concurrently -m 1 \"npm:clean\" \"npm:build\"",
    "clean": "rimraf \"*.tgz\" \".eslintcache\" \"dist\"",
    "lint": "npm run -s lint:eslint:check && npm run -s lint:prettier:check",
    "lint:eslint:check": "eslint --cache --format codeframe \"./**/*\"",
    "lint:eslint:fix": "npm run -s lint:eslint:check -- --fix",
    "lint:fix": "npm run -s lint:eslint:fix && npm run -s lint:prettier:fix",
    "lint:prettier:check": "npm run -s prettier -- -cu",
    "lint:prettier:fix": "npm run -s prettier -- -uw",
    "prepack": "npm run -s build:re",
    "prepare": "husky install",
    "prettier": "prettier --cache --loglevel=warn \"./**/*\"",
    "start": "echo \"Dry run: start\"",
    "test": "npm run -s lint"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.1",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-editorconfig": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "prettier-plugin-sh": "^0.12.8",
    "prettier-plugin-sort-json": "^0.0.3",
    "pretty-quick": "^3.1.3",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2",
    "typescript-eslint-language-service": "^5.0.0"
  },
  "engines": {
    "node": ">=8.18.0",
    "npm": ">=8.18.0"
  }
}
