{
  "name": "dantalion-web-demo",
  "version": "0.20.0",
  "private": true,
  "description": "The playground web app that calculates the personality from the birthday",
  "keywords": [
    "ba-zi",
    "bazi",
    "birthday",
    "chinese-astrology",
    "console",
    "console-app",
    "four-pillars",
    "library",
    "self-assessment",
    "terminal-app",
    "ziweidoushu",
    "zodiac"
  ],
  "homepage": "https://kurone-kito.github.io/dantalion/",
  "bugs": {
    "url": "https://github.com/kurone-kito/dantalion-web-demo/issues"
  },
  "repository": "github:kurone-kito/dantalion-web-demo",
  "license": "MIT",
  "author": "kurone-kito <krone@kit.black> (https://kit.black/)",
  "main": "./dist/index.html",
  "files": [
    ".next",
    "dist"
  ],
  "scripts": {
    "build": "concurrently -m 1 \"npm:build:storybook\" \"npm:build:next\"",
    "build:next": "next build && next export -o dist",
    "build:re": "concurrently -m 1 \"npm:clean\" \"npm:build\"",
    "build:storybook": "build-storybook --modern -o public/storybook",
    "clean": "rimraf \"*.tgz\" \".eslintcache\" \".next\" \"dist\" \"public/storybook\"",
    "lint": "npm run -s lint:eslint:check && npm run -s lint:prettier:check",
    "lint:eslint:check": "eslint --cache --format codeframe \"./**/*\"",
    "lint:eslint:fix": "npm run -s lint:eslint:check -- --fix",
    "lint:fix": "npm run -s lint:eslint:fix && npm run -s lint:prettier:fix",
    "lint:prettier:check": "npm run -s prettier -- -cu",
    "lint:prettier:fix": "npm run -s prettier -- -uw",
    "prepack": "npm run -s build:re",
    "prepare": "husky install",
    "prettier": "prettier --cache --loglevel=warn \"./**/*\"",
    "serve": "serve dist",
    "start": "concurrently -k \"npm:watch:*\"",
    "test": "jest && npm run -s lint",
    "watch:next": "next dev",
    "watch:storybook": "start-storybook -p 6006 --ci --modern"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
