    "classnames": "^2.3.1",
    "next": "^12.2.5",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-i18next": "^11.15.3",
    "react-markdown": "^8.0.3",
    "tailwindcss": "^3.1.8",
    "twin.macro": "^2.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.1",
    "@commitlint/config-conventional": "^17.1.0",
    "@emotion/babel-plugin": "^11.10.2",
    "@emotion/babel-preset-css-prop": "^11.10.0",
